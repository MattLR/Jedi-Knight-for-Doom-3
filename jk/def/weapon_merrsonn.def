	"smoke_residue"				""						// particle effect while in the air
	"smoke_bounce"				""
	"smoke_fuse"				""

	"debris_count"				"6"
	"def_debris"				"debris_largeshrapnel"
	"def_shrapnel"				"debris_shrapnel"
	
	"mtr_light_shader"			"muzzleflash"
